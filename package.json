{
  "name": "full-stack-nodejs-react-app",
  "version": "1.0.0",
  "type": "module",
  "description": "A full-stack NodeJs and React project",
  "scripts": {
    "test": "npm run test:vitest && npm run test:codecept",
    "test:vitest": "vitest run --coverage",
    "test:codecept": "echo 'Not implemented'",
    "dev:server": "vite build && nodemon --esm src/index.ts",
    "dev:client": "vite",
    "build": "rimraf dist && tsc -p tsconfig.build.json && vite build",
    "strip": "dependency-strip --update --path=dist/**/*.js"
  },
  "keywords": [
    "TypeScript",
    "NodeJs",
    "ReactJs"
  ],
  "author": "Stefan Hilhorst",
  "license": "MIT",
  "dependencies": {
    "@types/bcryptjs": "^2.4.4",
    "bcryptjs": "^2.4.3",
    "compression": "1.7.4",
    "dotenv": "^16.0.3",
    "express": "4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.26",
    "@types/supertest": "^2.0.12",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-v8": "^0.34.6",
    "dependency-strip": "^1.0.5",
    "nodemon": "^2.0.21",
    "rimraf": "^4.4.0",
    "sass": "^1.59.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vitest": "^0.34.6"
  }
}
